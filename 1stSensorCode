int soundPin = A5;
int ledPin = 12;
int sensorValue = 0;
 
void setup () 
{
  Serial.begin (9600);
  pinMode (ledPin, OUTPUT);
}
 
void loop () 
{
  sensorValue = analogRead (soundPin);
  //if sensor goes above max light led (could be buzzer)
  if(sensorValue > 75)
  {
    Serial.println (sensorValue, DEC);
    digitalWrite (ledPin, HIGH);
    delay (50);
  }
  //switch off LED
  digitalWrite (ledPin, LOW);
  delay (50);
}
